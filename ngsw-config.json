{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.svg",
          "/**/*.png",
          "/**/*.jpg",
          "/**/*.jpeg",
          "/**/*.gif",
          "/**/*.webp",
          "/**/*.avif",
          "/**/*.ico",
          "/**/*.woff",
          "/**/*.woff2",
          "/**/*.ttf",
          "/**/*.otf"
        ]
      }
    }
  ]
}
